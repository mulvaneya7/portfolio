<div class="project-hub">
    <div class="project-hub__header">
        <h1>Projects</h1>
    </div>
    <div class="project-hub__display">
        {{#each this.projects as |project|}}
            <ProjectMap::Project::ProjectTile @title={{project.title}} @description={{project.description}} @project-name={{project.title}}/>
        {{/each}}
        
    </div>
</div>